{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "comgateTransId": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "comgatePaymentStatus": {
      "type": "string",
      "default": "PENDING",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "cutleryAmount": {
      "type": "integer",
      "required": true
    },
    "products": {
      "type": "json",
      "required": true
    },
    "additionals": {
      "type": "json",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "note": {
      "type": "string"
    },
    "modifiers": {
      "type": "json",
      "required": false
    },
    "deliveryPrice": {
      "type": "integer",
      "required": false
    },
    "paymentType": {
      "type": "enumeration",
      "enum": [
        "card",
        "cash",
        "cardPickup",
        "cardCourier"
      ],
      "required": true
    },
    "dayCreated": {
      "type": "string"
    }
  }
}
