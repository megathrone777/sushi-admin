{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "comgateTransId": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "comgatePaymentStatus": {
      "type": "string",
      "default": "PENDING",
      "required": true
    },
    "phone": {
      "type": "integer",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    }
  }
}
